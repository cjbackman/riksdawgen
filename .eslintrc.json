{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:import/errors"
  ]
  ,"parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  }
  ,"env": {
    "browser": true,
    "es6": true
  }
  ,"plugins": [
    "react",
    "import"
  ]
  ,"rules": {
    "no-console": 0
  }
