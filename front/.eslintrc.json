{
  "parser": "babel-eslint",
  "extends": ["standard", "standard-jsx", "standard-react"],
  "env": {
    "browser": true,
    "es6": true,
    "jest": true
  },
  "globals": {
    "process": false,
    "global": false
  },
  "rules": {
    "jsx-quotes": ["error", "prefer-double"],
    "space-before-function-paren": ["error", { "named": "never" }]
  }
}
